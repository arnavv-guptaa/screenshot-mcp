screenshot_report.json
report.json
*.report

# Playwright specific
test-results/
playwright-report/
playwright/.cache/

# Chrome/Chromium specific
chrome_crashpad_handler
chrome_debug.log
*.crash

# Session and authentication files
*.session
auth.json
credentials.json

# Local development files
.local
local/ 